E_0x55b91c13d330 .event posedge, v0x55b91c13cde0_0;
    .scope S_0x55b91c13cbc0;
T_0 ;
    %wait E_0x55b91c13d330;
    %load/vec4 v0x55b91c14e8f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55b91c14e720_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55b91c14e800_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %load/vec4 v0x55b91c14e720_0;
    %assign/vec4 v0x55b91c14e720_0, 0;
    %jmp T_0.7;
T_0.2 ;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x55b91c14e720_0, 0;
    %jmp T_0.7;
T_0.3 ;
    %pushi/vec4 2, 0, 4;
    %assign/vec4 v0x55b91c14e720_0, 0;
    %jmp T_0.7;
T_0.4 ;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x55b91c14e720_0, 0;
    %jmp T_0.7;
T_0.5 ;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x55b91c14e720_0, 0;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55b91c1050d0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x55b91c14ea60_0;
    %inv;
    %store/vec4 v0x55b91c14ea60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55b91c1050d0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b91c14ea60_0, 0, 1;
