S_0x55b91c1050d0 .scope module, "testbench" "testbench" 2 3;
 .timescale -9 -12;
P_0x55b91c105250 .param/l "CYCLE" 0 2 17, +C4<00000000000000000000000000001010>;
v0x55b91c14ea60_0 .var "clk_n", 0 0;
