S_0x55b91c13cbc0 .scope module, "test" "exponential" 2 10, 3 1 0, S_0x55b91c1050d0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "reset_n"
    .port_info 1 /INPUT 1 "clk_i"
    .port_info 2 /INPUT 2 "num"
    .port_info 3 /OUTPUT 4 "exp"
v0x55b91c13cde0_0 .net "clk_i", 0 0, v0x55b91c14ea60_0;  1 drivers
